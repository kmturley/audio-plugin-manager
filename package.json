{
  "name": "@studiorack/cli",
  "version": "1.2.1",
  "description": "Audio project manager tool",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "ts-node src/index.ts",
    "dev:cli": "rm /usr/local/bin/studiorack && npm run build && npm link",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json"
  },
  "bin": {
    "studiorack": "dist/index.js"
  },
  "keywords": [
    "studiorack",
    "project",
    "manager",
    "tool",
    "vst",
    "plugin",
    "daw"
  ],
  "author": "kmturley",
  "license": "ISC",
  "dependencies": {
    "@studiorack/core": "^1.2.3",
    "cli-table3": "^0.6.0",
    "commander": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^14.14.41",
    "jest": "^28.1.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studiorack/studiorack-cli.git"
  },
  "bugs": {
    "url": "https://github.com/studiorack/studiorack-cli/issues"
  },
  "homepage": "https://github.com/studiorack/studiorack-cli#readme"
}
